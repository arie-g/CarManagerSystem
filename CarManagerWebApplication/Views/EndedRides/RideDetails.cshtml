@model KeyValuePair<CarManagerWebApplication.Models.RideInfoSum, List<Dal.DrivePackage>>


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-horizontal">
    <table class="table table-striped table-hover ">
        <h2>
            Ride Summary:
        </h2>
        <thead>
            <tr style="font-size:18px">
                <th>Ride Start Time</th>
                <th>Ride End Time</th>
                <th>Car Licence Number</th>
                <th>Car Model</th>
                <th>Driver Name</th>
                <th>Average Speed</th>
                <th>High Engine Temp</th>
                <th>High RPM</th>
            </tr>
        </thead>
        <tr class="danger">
            <td>
                @Html.DisplayFor(modelItem => Model.Key.StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.CarLicence)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.CarModel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.DriverName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.Speed)
            </td>
            <td>c
                @Html.DisplayFor(modelItem => Model.Key.Temp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Key.RPM)
            </td>
            <td>
                
                
                
                
                
                

            </td>
        </tr>
        
        
        <thead>

            <td>

                Ride Details:
            </td>
            <tr style="font-size:18px">
                <th>Ride Id</th>
                <th>Time</th>
                <th>Speed</th>
                <th>Rpm</th>
                <th>Engine Temp</th>
            </tr>
        </thead>
                @foreach (var package in Model.Value)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => package.RideId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => package.Time)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => package.Speed)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => package.RPM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => package.EngineTemp)
                    </td>
                </tr>
                }
        </table>
</div>


